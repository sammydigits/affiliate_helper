{
  "manifest_version": 2,

  "name": "Affiliate Helper",
  "description": "Easily search for this product on an affiliate site",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Affiliate Helper"
  },
  "permissions": [
    "activeTab"
  ]
}